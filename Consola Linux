# COMANDOS UTILES DE CONSOLA

cd 				                para ir a carpetas
cd .. 				            para salir de carpetas
mkdir nombreCarpeta		    para crear carpetas
ls				                para ver carpetas y archivos
clear			              	para borrar la terminal
touch nombreArchivo.txt		para crear archivos
rm nombreArchivo.txt		  para borrar archivos
rm -rf nombreCarpeta		  para borrar carpetas
